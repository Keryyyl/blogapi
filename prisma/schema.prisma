generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int      @id @default(autoincrement())
  title     String
  content   String
  category  String
  createdAt DateTime @map("created_at")
  updatedAt DateTime? @map("updated_at")

  @@map("post")
}
